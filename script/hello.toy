for (var i = 0; i < 10; i++) {
	print i;
	if (i >= 7) {
		continue;
	}
	print "end of the loop";
}

print "-----";

var str = "";

for (var i = 0; i < 10; i++) {
	str = str + "x";
	print str;
}

